{
  "name": "noac-frontend",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --host 0.0.0.0 --port 4300",
    "start:ngrok": "ng serve --host 0.0.0.0 --port 4300 --disableHostCheck true",
    "build": "ng build",
    "build:prod": "ng build --prod",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "docker:init": "docker-compose up -d",
    "docker:build": "docker-compose build",
    "docker:build:hard": "docker-compose build --no-cache",
    "docker:rebuild": "npm run docker:rm && npm run docker:build && npm run docker:init",
    "docker:rebuild:hard": "npm run docker:rm:hard && npm run docker:build:hard && npm run docker:init",
    "docker:rm": "docker-compose down",
    "docker:rm:hard": "docker-compose down --remove-orphans",
    "docker:recreate": "docker-compose up -d --force-recreate --no-deps noac-frontend",
    "build:ssr": "npm run build:client-and-server-bundles && npm run webpack:server",
    "serve:ssr": "node dist/server",
    "build:client-and-server-bundles": "npm run build:prod && ng run noac-frontend:server:production",
    "webpack:server": "webpack --config webpack.server.config.js --progress --colors"
  },
  "private": true,
  "dependencies": {
    "@agm/core": "^1.0.0-beta.6",
    "@angular/animations": "^7.1.4",
    "@angular/cdk": "^7.2.1",
    "@angular/common": "~7.1.0",
    "@angular/compiler": "~7.1.0",
    "@angular/core": "~7.1.0",
    "@angular/forms": "~7.1.0",
    "@angular/http": "^7.2.15",
    "@angular/material": "^7.2.1",
    "@angular/platform-browser": "~7.1.0",
    "@angular/platform-browser-dynamic": "~7.1.0",
    "@angular/platform-server": "^7.2.15",
    "@angular/router": "~7.1.0",
    "@ng-toolkit/universal": "^7.1.2",
    "@nguniversal/express-engine": "^7.1.1",
    "@nguniversal/module-map-ngfactory-loader": "^7.1.1",
    "@nicky-lenaers/ngx-scroll-to": "^2.0.0",
    "aos": "^2.3.4",
    "bootstrap": "^4.1.3",
    "compression": "^1.7.4",
    "core-js": "^2.5.4",
    "domino": "^2.1.3",
    "express": "^4.16.4",
    "hammerjs": "^2.0.8",
    "ngx-bootstrap": "^3.2.0",
    "ngx-stripe": "^7.2.0",
    "node-sass": "^4.11.0",
    "paymentfont": "^1.2.5",
    "popper.js": "^1.14.7",
    "rxjs": "~6.3.3",
    "tslib": "^1.9.0",
    "xmlhttprequest": "^1.8.0",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.13.0",
    "@angular/cli": "~7.1.1",
    "@angular/compiler-cli": "~7.1.0",
    "@angular/language-service": "~7.1.0",
    "@types/aos": "^3.0.1",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "~4.5.0",
    "cz-conventional-changelog": "^2.1.0",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~3.1.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "nodemon": "^1.18.10",
    "protractor": "~5.4.0",
    "ts-loader": "^6.0.0",
    "ts-node": "^7.0.1",
    "tslint": "~5.11.0",
    "typescript": "~3.1.6",
    "webpack-cli": "^3.3.2"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
